{
  "name": "user-js",
  "description": "My User scripts",
  "author": "JenieX",
  "type": "module",
  "scripts": {
    "watch": "tsx ./tasks/index.ts --watch",
    "build": "tsx ./tasks/index.ts",
    "build-docs": "tsx ./tasks/docs.ts",
    "serve": "tsx utils/remove-build.ts && rollup --config --configPlugin typescript && node build/user-js-server.js",
    "stage": "ngit-cli add",
    "commit": "npm run stage && start cmd /k \"git status && cd node_modules/.bin && commit -h"
  },
  "homepage": "https://github.com/JenieX/user-js",
  "bugs": "https://github.com/JenieX/user-js/issues",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/prompt-cli": "^17.6.3",
    "@jeniex/eslint-config-js": "^0.0.3",
    "@jeniex/eslint-config-ts": "^0.0.9",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/eslint": "^8.37.0",
    "@types/greasemonkey": "^4.0.4",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.16.3",
    "chalk": "^5.2.0",
    "cssnano": "^6.0.1",
    "eslint": "^8.39.0",
    "htmlnano": "^2.0.4",
    "husky": "^8.0.3",
    "inquirer": "^9.2.2",
    "magic-string": "^0.30.0",
    "ngit-cli": "^1.2.0",
    "paint-console": "^0.0.1",
    "postcss": "^8.4.23",
    "posthtml": "^0.16.6",
    "rollup": "^3.21.4",
    "rollup-plugin-external-globals": "^0.7.3",
    "sass": "^1.62.1",
    "terser": "^5.17.1",
    "tslib": "^2.5.0",
    "tsx": "^3.12.7",
    "watcher": "^2.2.2"
  }
}
