{
  "files.exclude": {
    ".husky": true,
    "build": true,
    "dist/**/*.css": true,
    "dist/**/*.html": true,
    "dist/**/*.meta.js": true,
    "dist/**/*.raw.js.map": true,
    "node_modules": true,
    // "tasks": true,
    "utils": true
  },
  // "[javascript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "[typescript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "code-runner.executorMap": { "typescript": "tsx" },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "testing.js": "testing.*.ts, testing.*.js",
    "package.json": ".gitignore, config.json, tsconfig-rollup-config.json, tsconfig.json, .commitlint*, .eslint*, .prettier*, package-lock.json, rollup.config.*"
  }
}
