{
  "files.exclude": {
    ".husky": true,
    "build": true,
    "dist/**/*.css": true,
    "dist/**/*.html": true,
    "dist/**/*.meta.js": true,
    "dist/**/*.raw.js.map": true,
    "dummy.js": true,
    "node_modules": true,
    "src/lb-common-films": true,
    "src/lb-common-films-alt": true,
    "src/lb-common-films-b": true
  },
  "[typescript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "code-runner.executorMap": { "typescript": "tsx" },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "package.json": ".commitlint*, .eslint*, .gitignore, .nested.json, .prettier*, config.json, package-lock.json, rollup.config.*, scrap.ts, to-do.md, tsconfig-rollup-config.json, tsconfig.*.json, tsconfig.json"
  }
}
