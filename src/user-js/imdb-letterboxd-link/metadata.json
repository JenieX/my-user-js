{
  "name": "imdb-letterboxd-link",
  "version": "0.0.1",
  "description": "Provides the corresponding Letterboxd links of movies if available.",
  "match": ["https://www.imdb.com/title/*/*"],
  "runAt": "document-start",
  "icon": "https://www.google.com/s2/favicons?sz=64&domain=www.imdb.com",
  "grant": ["GM.xmlHttpRequest", "GM.getResourceUrl"],
  "resource": [
    "letterboxdLogo https://github.com/JenieX/user-js/blob/main/src/user-js/imdb-letterboxd-link/assets/sprite.svg?raw=true"
  ]
}
